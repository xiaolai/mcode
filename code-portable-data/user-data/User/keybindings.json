// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+tab",
        "command": "leaper.leap",
        "when": "editorTextFocus && leaper.hasLineOfSight && leaper.inLeaperMode && !editorHasSelection && !editorTabMovesFocus && !suggestWidgetVisible"
    },
    {
        "key": "tab",
        "command": "-leaper.leap",
        "when": "editorTextFocus && leaper.hasLineOfSight && leaper.inLeaperMode && !editorHasSelection && !editorTabMovesFocus && !suggestWidgetVisible"
    }
]